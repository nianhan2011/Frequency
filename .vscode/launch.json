        {
            "cwd": "${workspaceRoot}",
            "type": "cortex-debug",
            "request": "launch",
            "name": "jlink",
            "servertype": "jlink",
            "interface": "swd",
            "executable": "build\\Frequency\\Frequency.elf",
            "runToEntryPoint": "main",
            "device": "null"
        }
    ]
}